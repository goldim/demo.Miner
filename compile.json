{
  "$schema": "https://qooxdoo.org/schema/compile-1-0-0.json",
  "targets": [
    {
      "type": "source",
      "outputPath": "compiled/source",
      "bundle": {
        "include": "qx.*"
      }
    },
    {
      "type": "build",
      "outputPath": "compiled/build"
    }
  ],
  "defaultTarget": "source",
  "locales": ["en","ru"],

  "applications": [
    {
      "class": "minesweeper.demo.Application",
      "theme": "qx.theme.Classic",
      "name": "minesweeper"
    }
  ]
}
